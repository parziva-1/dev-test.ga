
head
    meta(http-equiv='content-type', content='text/html; charset=utf-8')
    meta(content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0', name='viewport')
    meta(content='yes', name='apple-mobile-web-app-capable')
    meta(content='black-translucent', name='apple-mobile-web-app-status-bar-style')
    meta(content='telephone=no', name='format-detection')
    
    meta(property="og:type" content="website")
    meta(property="og:url" content="https://dev-test.ga")
    meta(property="og:image" content="https://dev-test.ga/android-icon-192x192.png")
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@Parziva_1">
    <link rel="canonical" href="https://dev-test.ga/" />
    
    if is_home()
      meta(property="og:description" content=config.description)
      meta(name='description', content=config.description)
      meta(name="twitter:description" content=config.description)
      meta(property="og:title" content=config.title+" | "+config.description)
      meta(name="twitter:title" content=config.title+" | "+config.description)
      
    else if is_post()
      - var des = config.description+", "+page.desc + '...'
      meta(property="og:description" content=des)
      meta(property="og:title" content=page.title+" | "+config.title)
      meta(name='description', content=des)
      meta(name="twitter:description" content=des)
      meta(name="twitter:title" content=page.title+" | "+config.title)
    else
      meta(property="og:description" content=config.description)
      meta(name='description', content=config.description)
      meta(name="twitter:description" content=config.description)
      meta(property="og:title" content=config.title+" | "+config.description)
      meta(name="twitter:title" content=config.title+" | "+config.description)

